<http://geontime.com/geonkick#author>
	a foaf:Person ;
	foaf:name "Iurie Nistor" ;
	foaf:mbox <mailto:nistor@iurie.org> ;
	foaf:homepage <http://iurie.org/> .

<http://geontime.com/geonkick>
	a doap:Project ;
	doap:maintainer <http://geontime.com/geonkick#author> ;
	doap:name "Geonkick" .

